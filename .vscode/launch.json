{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "args": ["--multiple", "10", "--hill"],
            "python": "${workspaceFolder}/venv/bin/python"
        }
    ]
}
